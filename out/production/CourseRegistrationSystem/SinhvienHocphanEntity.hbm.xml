<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.SinhvienHocphanEntity" table="sinhvien_hocphan" schema="dangkihocphan">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="idSinhVien">
            <column name="idSinhVien" sql-type="int"/>
        </property>
        <property name="idHocPhanMo">
            <column name="idHocPhanMo" sql-type="int"/>
        </property>
        <property name="ngayDangKi">
            <column name="ngayDangKi" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>